#!/bin/bash
#SBATCH --nodes=1
#SBATCH --cpus-per-task=32
#SBATCH --time=10-48:00:00
#SBATCH --mem=128000mb
#SBATCH --gres=gpu:1
#SBATCH --job-name=tf_random_classifier

# --time=30
# --time=48:00:00

export PARNODES="$SLURM_CPUS_PER_TASK"
